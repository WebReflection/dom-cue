{
  "type": "module",
  "license": "MIT",
  "description": "A minimalistic signals implementation for vanilla DOM/JS",
  "keywords": [
    "signals",
    "reactive",
    "dom",
    "javascript"
  ],
  "author": "Andrea Giammarchi",
  "name": "dom-cue",
  "version": "0.0.7",
  "main": "index.js",
  "module": "index.js",
  "files": [
    "index.js",
    "dist.js",
    "README.md",
    "LICENSE"
  ],
  "exports": {
    ".": {
      "import": "./index.js"
    },
    "./dist": {
      "import": "./dist.js"
    }
  },
  "scripts": {
    "build": "rollup --config build/index.js && npm run test && npm run size",
    "test": "node test/index.js",
    "size": "cat dist.js | brotli | wc -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/WebReflection/dom-cue.git"
  },
  "bugs": {
    "url": "https://github.com/WebReflection/dom-cue/issues"
  },
  "homepage": "https://github.com/WebReflection/dom-cue#readme",
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "rollup": "^4.52.4"
  }
}
